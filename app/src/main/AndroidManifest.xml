<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.martin.cimbrecompiled">


    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.USE_BIOMETRIC"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/cimblogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cimblogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".PictureActivity"></activity>

        <activity android:name=".ConfirmGestureActivity"></activity>

        <activity android:name=".PictureLoginActivity"></activity>

        <activity android:name=".LoginSuccessActivity"></activity>

        <activity android:name=".PostActivity"></activity>

        <activity android:name=".SuccessfulSetup"></activity>

        <activity android:name=".FingerprintActivity"></activity>

        <activity android:name=".EnhancedLoginSelection"></activity>

        <activity android:name=".FPLogin"></activity>

        //change activity name
        <activity android:name=".TnCActivity"></activity>
        <activity android:name=".PermissionsActivity"></activity>
        <activity android:name=".OTPActivity"></activity>
    </application>

</manifest>

